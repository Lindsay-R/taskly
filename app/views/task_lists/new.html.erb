<h2>Add a task list</h2>

<!--url: new_path,-->
<%= form_for @task_list,  html: { class: "signin-form" } do |f| %>

  <% f.object.errors.full_messages.each do |error| %>
    <div class="errors">
      <%= error %>
    </div>
  <% end %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.submit "Create Task List"  %>
  </div>

<% end %>